{
  "cli": {
    "version": ">= 3.15.1",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
      
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
        "env": {
          "API_KEY": "your-api-key"
        },
        "android": {
          "buildType": "app-bundle"
        }
      }
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "androidApk": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  },
}